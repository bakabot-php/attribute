{
    "name": "nayleen/attribute",
    "description": "Provides accessors to single-value PHP Attributes.",
    "license": "MIT",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "nayleen/development": "^2.0",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Nayleen\\Attribute\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nayleen\\Attribute\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "lock": false,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.1.x-dev"
        }
    }
}
