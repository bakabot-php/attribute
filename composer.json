{
    "name": "bakabot/attribute",
    "config": {
        "lock": false,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Bakabot\\Attribute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bakabot\\Attribute\\": "tests/"
        }
    }
}
